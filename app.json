{
  "expo": {
    "name": "PRICE.AI",
    "slug": "price-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/final_quest_240x240.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/final_quest_240x240.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e4a72"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/final_quest_240x240.png",
        "backgroundColor": "#1e4a72"
      }
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "name": "PRICE.AI - Smart Pricing Assistant",
      "shortName": "PRICE.AI",
      "lang": "en",
      "scope": "/",
      "themeColor": "#1e4a72",
      "backgroundColor": "#1e4a72",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}